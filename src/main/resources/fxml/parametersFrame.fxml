            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Circle fill="#f88cd7" layoutX="724.0" layoutY="30.0" onMouseClicked="#hideParametersPane" radius="22.0" stroke="BLACK" strokeType="INSIDE">
