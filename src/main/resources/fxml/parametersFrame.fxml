         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>

      <Button alignment="CENTER" layoutX="314.0" layoutY="642.0" mnemonicParsing="false" onAction="#disconnectClick" prefHeight="37.0" prefWidth="131.0" styleClass="btn_disconnect" text="DISCONNECT">
