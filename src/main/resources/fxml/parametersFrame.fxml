<AnchorPane prefHeight="753.0" prefWidth="753.0" styleClass="parameters" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ParametersFrame">
   <children>
      <TextField fx:id="textFieldPseudo" alignment="CENTER" layoutX="201.0" layoutY="383.0" prefHeight="55.0" prefWidth="348.0" promptText="Enter your new pseudo" styleClass="field_pseudo" />
